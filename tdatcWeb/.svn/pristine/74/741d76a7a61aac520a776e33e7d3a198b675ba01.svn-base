function isNumber(evt) {
    evt = (evt) ? evt : window.event;
    var phone= $("#phone").val();
  
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    else if(phone.length>9){
    	return false;
    }
    return true;
}

function ispinNumber(evt) {
    evt = (evt) ? evt : window.event;
    var pinCode = $("#pinCode").val();
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    else if(pinCode.length>5){
    	return false;
    }
    return true;
}


function ispoppinNumber(evt) {
    evt = (evt) ? evt : window.event;
    var pop_pinCode = $("#pop_pinCode").val();
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    else if(pop_pinCode.length>5){
    	return false;
    }
    return true;
}


function isfaxNumber(evt) {
    evt = (evt) ? evt : window.event;
    var fax = $("#fax").val();
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    else if(fax.length>30){
    	return false;
    }
    return true;
}





