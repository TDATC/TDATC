function isNumber(evt) {
    evt = (evt) ? evt : window.event;
    var ph1 = $("#ph1").val();
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    else if(ph1.length>9){
    	return false;
    }
    return true;
}

function isfaxNumber(evt) {
    evt = (evt) ? evt : window.event;
    var fax = $("#fax").val();
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    else if(fax.length>30){
    	return false;
    }
    return true;
}





function ispinNumber(evt) {
    evt = (evt) ? evt : window.event;
    var pincode1 = $("#pincode1").val();
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    else if(pincode1.length>5){
    	return false;
    }
    return true;
}

